{
  "name": "re-reduced",
  "version": "0.4.0",
  "description": "A utility toolbelt that reduces boilerplate from your react/redux app",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanrsoares/re-reduced.git"
  },
  "keywords": [
    "redux",
    "higer-order-reducer",
    "react",
    "immutable",
    "functional programming"
  ],
  "author": "Alan Soares <alanrsoars@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanrsoares/re-reduced/issues"
  },
  "homepage": "https://github.com/alanrsoares/re-reduced#readme",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/ramda": "^0.25.39",
    "@types/react-redux": "^6.0.9",
    "docz": "^0.12.5",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.2"
  }
}
